{
  "name": "smartbooster/parameter-bundle",
  "type": "symfony-bundle",
  "description": "SMART BOOSTER parameter bundle",
  "keywords": [],
  "homepage": "https://github.com/smartbooster/parameter-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Ducrot",
      "email": "mathieu.ducrot@smartbooster.io",
      "homepage": "https://www.smartbooster.io"
    }
  ],
  "require": {
    "php": "^7.4",
    "symfony/http-kernel": "^4.4 || ^5.1.5",
    "symfony/config": "^4.4 || ^5.1",
    "symfony/yaml": "^4.4 || ^5.1",
    "symfony/dependency-injection": "^4.4 || ^5.1",
    "doctrine/orm": "^2.9",
    "doctrine/dbal": "^2.9",
    "doctrine/doctrine-bundle": "^2.4",
    "smartbooster/sonata-bundle": "^1.1-dev"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "sebastian/phpcpd": "^6.0",
    "phpmetrics/phpmetrics": "^2.7",
    "phpstan/phpstan": "^0.12.90",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan-symfony": "^0.12.37",
    "symfony/framework-bundle": "^4.4 || ^5.1",
    "phpstan/phpstan-doctrine": "^0.12.39",
    "dama/doctrine-test-bundle": "^6.6",
    "liip/test-fixtures-bundle": "^2.0",
    "theofidry/alice-data-fixtures": "^1.4",
    "doctrine/doctrine-fixtures-bundle": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Smart\\ParameterBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\ParameterBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
